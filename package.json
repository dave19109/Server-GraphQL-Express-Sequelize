{
  "name": "repository",
  "version": "1.0.0",
  "description": "Template for web app",
  "main": "index.js",
  "scripts": {
    "migrations": "sequelize db:migrate --migrations-path ./src/migrations --config ./src/configs/database.json",
    "undo": "sequelize db:migrate:undo:all --migrations-path ./src/migrations --config ./src/configs/database.json",
    "seeders": "sequelize db:seed:all --seeders-path ./src/seeders --config ./src/configs/database.json",
    "start": "nodemon --exec babel-node --extensions '.ts,.tsx' --plugins require-context-hook --watch ./ ./src/index.ts",
    "build": "webpack --config webpack.server.build.config.js",
    "production": "node dist/server/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "dave",
  "license": "ISC",
